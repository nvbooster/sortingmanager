{
    "name" : "nvbooster/sortingmanager",
    "type" : "library",
    "license" : "MIT",
    "description" : "Sorting Manager for SQL/Doctrine",
    "keywords" : [
        "sorting helper ui"
    ],
    "authors" : [{
            "name" : "Vyacheslav Nechipurenko",
            "email" : "nvb@aproxima.ru",
            "homepage" : "https://github.com/nvbooster"
    }],
    "autoload" : {
        "psr-4" : {
            "nvbooster\\SortingManager\\" : "src"
        }
    },
    "require" : {
        "php" : ">=5.5",
        "symfony/options-resolver" : "~2.5.0",
        "symfony/http-foundation" : "~2.5.0"
    },
    "suggest" : {
        "doctrine/orm" : "For using DoctrineORMQueryBuilderHandler"
    },
    "minimum-stability" : "dev",
    "require-dev" : {
        "phpunit/phpunit" : "5.6.x-dev",
        "doctrine/orm" : ">=2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }    
}